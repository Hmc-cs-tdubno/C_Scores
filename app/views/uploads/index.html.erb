<% if flash[:notice]=="Signed in successfully." %>
  
  <div class="alert alert-success">
    <strong>Success!</strong> <%=flash[:notice].html_safe%>.
  </div>
  <%elsif flash[:notice]%>
  <div class="alert alert-danger">
    <strong>Danger!</strong> <%=flash[:notice].html_safe%>.
  </div>
  <%= puts flash[:notice]%>
  <% end %>
<div>
  
<h4>Import that data!</h4>
  <%=form_tag import_uploads_path, multipart: true do %>
  <%= file_field_tag :file%>
  <%= submit_tag "Import CSV/JSON",{:class => 'btn btn-primary'} %>

  <% end %>
</div>
